algoritmo "creuza01"
var
   ano_atual: Inteiro
   ano_nascimento: Inteiro
   s: Inteiro
inicio
      Escreval ("Em que anos nós estamos? ")
      Leia (ano_atual)
      Escreval ("Em que ano eu nasci? ")
      Leia (ano_nascimento)
      s <- ano_atual - ano_nascimento
      Escreva ("Minha idade é de ", s)
fimalgoritmo

algoritmo "Creuza03"
var
   R, D, C, Q: Real
inicio
      c <- 1
      Escreval ("Quantas vezes você convertera? ")
      Leia (Q)
      Enquanto (c <=q) faca
               Escreva ("Qual é o valor em R$? ")
               Leia (R)
               D <- R/2.20

               Escreval ("O valor convertido er de US$ ", D:5:2)
               c <- c + 1
      FimEnquanto
fimalgoritmo
